<p id="notice"><%= notice %></p>

<h1>Skool Years</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Teacher</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skool_years.each do |skool_year| %>
      <tr>
        <td><%= skool_year.year %></td>
        <td><%= skool_year.teacher_id %></td>
        <td><%= link_to 'Show', skool_year %></td>
        <td><%= link_to 'Edit', edit_skool_year_path(skool_year) %></td>
        <td><%= link_to 'Destroy', skool_year, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skool Year', new_skool_year_path %>
